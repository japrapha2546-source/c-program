#include <stdio.h>
int main() {
    int n,s=0;
    printf("Enter days overdue: ");
    scanf("%d",&n);
    if(n>30){
        printf("Membership Cancelled\n");}
     else if(n>10){
        s =5* 2+ 5*5+(n-10)*10;
        printf("Fine: 5*2 + 5*5 + %d*10 = %d\n",n-10,s);}
     else if (n>5) {
        s = 5*2 + (n-5)*5;
        printf("Fine: 5*2 + %d*5 = %d\n", n-5,s);}
     else if (n>0) {
        s = n*2;
        printf("Fine: %d*2 = %d\n",n,s);}
     else {
        printf("No fine.\n");}
}
